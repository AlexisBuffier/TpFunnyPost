<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FunnyPostController">
   <bottom>
      <Pane prefHeight="498.0" prefWidth="918.0" BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="listPost" layoutX="39.0" layoutY="23.0" prefHeight="410.0" prefWidth="199.0" />
            <Label fx:id="titlePost" alignment="CENTER" layoutX="440.0" layoutY="82.0" prefHeight="21.0" prefWidth="296.0" text="Label">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <TextArea fx:id="bodyPost" editable="false" layoutX="341.0" layoutY="137.0" prefHeight="253.0" prefWidth="494.0" wrapText="true" />
            <Button fx:id="btnInit" layoutX="104.0" layoutY="446.0" mnemonicParsing="false" onMouseClicked="#insertAllPost" prefHeight="31.0" prefWidth="69.0" text="Init" />
            <Button fx:id="btnDelete" layoutX="680.0" layoutY="431.0" mnemonicParsing="false" onMouseClicked="#deletePost" text="Supprimer" />
            <TextField fx:id="inputId" layoutX="482.0" layoutY="431.0" />
            <Label layoutX="432.0" layoutY="436.0" text="Id :" />
            <Label fx:id="labelDelete" layoutX="482.0" layoutY="401.0" visible="false" />
            <Button fx:id="btnAccueil" layoutX="769.0" layoutY="23.0" mnemonicParsing="false" onMouseClicked="#onClickAccueil" prefHeight="43.0" prefWidth="109.0" text="Accueil" />
         </children>
      </Pane>
   </bottom>
</BorderPane>
